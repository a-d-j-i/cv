---
import {model} from "../model";
const {summary} = model;
---
<div class="card bg-base-100 border">
    <div class="card-body">
        <h2 class="card-title">Professional Summary</h2>
        <div class="leading-relaxed space-y-3">
            {summary.map(s => <p>{s}</p>)}
        </div>
    </div>
</div>

